{
  "checks": {
    "check-jenkins": {
      "handlers": [],
      "command": "check-jenkins.rb --server jenkins --port 8080 ",
      "interval": 5,
      "subscribers": [ "jenkins" ]
    }, 
    "check-jenkins-metrics": {
      "handlers": [],
      "command": "metrics-jenkins.rb --server jenkins --port 8080 ",
      "interval": 5,
      "subscribers": [ "jenkins" ]
    }, 
    "check-jenkins-job-status": {
      "handlers": [],
      "command": "check-jenkins-job-status.rb --url http://jenkins:8080 -v 3 --jobs 'test1'",
      "interval": 5,
      "subscribers": [ "jenkins" ]
    }, 
    "check-jenkins-health": {
      "handlers": [],
      "command": "check-jenkins-health.rb --server jenkins --port 8080 ",
      "interval": 5,
      "subscribers": [ "jenkins" ]
    }, 
    "check-jenkins-build-time": {
      "handlers": [],
      "command": "check-jenkins-build-time.rb --url http://jenkins:8080 --jobs 'test1=100'",
      "interval": 5,
      "subscribers": [ "jenkins" ]
    }
  }
}
